{
  "name": "eager-app-boilerplate",
  "version": "0.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "build": "NODE_ENV=production npm run lint && npm run build:js && npm run build:css",
    "build:css": "postcss --use autoprefixer -o build/app.css source/app.css",
    "build:js": "babel --out-dir build source",
    "lint": "eslint ./source",
    "watch:js": "babel --watch --out-dir build source",
    "watch:css": "postcss --watch --use autoprefixer -o build/app.css source/app.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EagerApps/AppBoilerplate.git"
  },
  "author": "Eager",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EagerApps/AppBoilerplate/issues"
  },
  "homepage": "https://github.com/EagerApps/AppBoilerplate#readme",
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "6.3.6",
    "babel-cli": "6.8.0",
    "babel-eslint": "6.0.4",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-stage-0": "6.5.0",
    "eslint": "2.9.0",
    "eslint-loader": "1.3.0",
    "eslint-plugin-babel": "3.2.0",
    "postcss-cli": "2.5.2"
  }
}
